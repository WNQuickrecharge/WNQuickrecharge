<?xml version="1.0" encoding="utf-8"?>
<com.weijing.materialanimatedswitch.view.BetterViewAnimator xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/trending_animator"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:inAnimation="@android:anim/fade_in"
    android:outAnimation="@android:anim/fade_out">

    <RelativeLayout
        android:id="@+id/rl_no_car"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <TextView
            android:id="@+id/tv_error"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true" />
    </RelativeLayout>

</com.weijing.materialanimatedswitch.view.BetterViewAnimator>